<?xml version='1.0' encoding='ASCII'?>
<CONFIG>
    <conn>
        <per_lcore_size></per_lcore_size>
    </conn>
    <stream_reassemble>
        <rsvd_pkts>
            <!-- per nic must gt 10000-->
            20000
        </rsvd_pkts>
    </stream_reassemble>
    <care_biz>yes</care_biz>
    <sample>
           <enable>yes</enable>
           <method>packet</method>
           <rate>16384</rate>
    </sample>
    <dpi>
        <protocol>
            <name>http</name>
            <enable>no</enable>
            <parse>no</parse>
        </protocol>
        <protocol>
            <name>dns</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>
        <protocol>
            <name>ntp</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>
        <protocol>
            <name>ssdp</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>
        <protocol>
            <name>memcache</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>     
        <protocol>
            <name>chargen</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>     
        <protocol>
            <name>qotd</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>     
        <protocol>
            <name>snmp</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>
        <protocol>
            <name>cldap</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>     
        <protocol>
            <name>tftp</name>
            <enable>yes</enable>
            <parse>no</parse>
        </protocol>     
    </dpi>
</CONFIG>
